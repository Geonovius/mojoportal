<?xml version="1.0" encoding="utf-8"?>
<SiteCreatedEventHandlers>
	<providers>
		<add name="DoNothingSiteCreatedEventHandler"
			type="mojoPortal.Business.WebHelpers.SiteCreatedEventHandlers.DoNothingSiteCreatedEventHandler, mojoPortal.Business.WebHelpers"
			description="This is a placeholder, do not delete this one. There must always be at least on provider in the collection. This one is not used but is here for medium trust issues." />
		<add name="UpdateSiteCountCacheEventHandler"
			type="mojoPortal.Web.Components.EventHandlers.SiteCreated.UpdateSiteCountCacheEventHandler, mojoPortal.Web"
			description="This updates the 'SiteCount' in-memory cache when a new site is created." />
		<add name="UpdateSiteCountCacheEventHandler"
			type="mojoPortal.Web.Components.EventHandlers.SitePreDelete.UpdateSiteCountCacheEventHandler, mojoPortal.Web"
			description="This updates the 'SiteCount' in-memory cache when a site is deleted." />
	</providers>
</SiteCreatedEventHandlers>
